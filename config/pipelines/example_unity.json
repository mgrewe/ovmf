{
	"webcam_input": {
	},

	"image_preview": {
		"receive":{
			"image_data": "webcam_input",
			"tracker_parameters": "openface_tracker"
		}
	},

	"openface_tracker": {
		"send_image": false,
		"receive":{
			"image_data": "webcam_input"
		}
	},

	"openface_remapper": {
		"send_image": false,
		"receive":{
			"image_data": "openface_tracker"
		}
	},

	"smoothing": {
		"receive":{
			"tracker_parameters": "openface_remapper"
		}
	},

	"expression_scaling": {
		"expressionscaling_au_scale": "1",
		"receive":{
			"tracker_parameters": "smoothing"
		}
	},

	"fexmm_remapper": {
		"receive" : {
			"tracker_parameters": "expression_scaling"
		}
	},

	"unity_renderer": {
		"receive": {
			"fexmm_parameters": "fexmm_remapper"
		}
	},

	"external_script": {
		"receive" : {
			"render_output": "unity_renderer"
		},
		"script": "modules/external/psychopy_example.py"
	}
}
